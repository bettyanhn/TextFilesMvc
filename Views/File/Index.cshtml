@{
    ViewData["Title"] = "Files Page";
    var files = ViewBag.FileName as string[];
}

<div class="text-center">
    <h1 class="display-4">@ViewData["Title"]</h1>
    @foreach (var file in files!)
    {
        <p>
          <a asp-area="" asp-controller="File"
          asp-action="Contents"
          asp-route-id="@file"
          >@file</a>
        </p>
    }
</div>
